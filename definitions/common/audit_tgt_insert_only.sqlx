config { type: "table"}
 
select 1 as sno,count(*) as total,
from (select *
from ${ref("asc_tgt003_patient_dimension_scd")} 
where last_updated in 
(select max(last_updated) 
from ${ref("asc_tgt003_patient_dimension_scd")})
and scd_valid_to is null
and  run_id = (select run_id from ${self()} ))

 


